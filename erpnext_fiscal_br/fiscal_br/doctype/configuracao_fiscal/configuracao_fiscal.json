            "fieldtype": "Section Break",
            "label": "Dados da Empresa"
        },
        {
            "fieldname": "empresa",
            "fieldtype": "Link",
            "in_list_view": 1,
            "label": "Empresa",
            "options": "Company",
            "reqd": 1,
            "unique": 1
        },
        {
            "fieldname": "cnpj",
            "fieldtype": "Data",
            "in_list_view": 1,
            "label": "CNPJ",
            "reqd": 1,
            "description": "CNPJ da empresa (apenas números)"
        },
        {
            "fieldname": "inscricao_estadual",
            "fieldtype": "Data",
            "label": "Inscrição Estadual",
            "reqd": 1
        },
        {
            "fieldname": "inscricao_municipal",
            "fieldtype": "Data",
            "label": "Inscrição Municipal"
        },
        {
            "fieldname": "column_break_empresa",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "regime_tributario",
            "fieldtype": "Select",
            "in_list_view": 1,
            "label": "Regime Tributário",
            "options": "1 - Simples Nacional\n2 - Simples Nacional - Excesso de Sublimite\n3 - Lucro Presumido\n4 - Lucro Real\n5 - Lucro Arbitrado\n6 - MEI - Microempreendedor Individual",
            "reqd": 1,
            "description": "Regime tributário da empresa conforme legislação fiscal"
        },
        {
            "fieldname": "tipo_apuracao",
            "fieldtype": "Select",
            "label": "Tipo de Apuração",
            "options": "\nTrimestral\nAnual",
            "depends_on": "eval:['3 - Lucro Presumido', '4 - Lucro Real'].includes(doc.regime_tributario)",
            "description": "Periodicidade de apuração do IRPJ e CSLL"
        },
        {
            "fieldname": "cnae",
            "fieldtype": "Data",
            "label": "CNAE",
            "description": "Código Nacional de Atividade Econômica"
        },
        {
            "fieldname": "codigo_uf",
            "fieldtype": "Data",
            "label": "Código UF (IBGE)",
            "reqd": 1,
            "description": "Ex: 35 para São Paulo"
        },
        {
            "fieldname": "codigo_municipio",
            "fieldtype": "Data",
            "label": "Código Município (IBGE)",
            "reqd": 1,
            "description": "Código IBGE do município (7 dígitos)"
        },
        {
            "fieldname": "section_impostos",
            "fieldtype": "Section Break",
            "label": "Configuração de Impostos",
            "depends_on": "eval:['3 - Lucro Presumido', '4 - Lucro Real', '5 - Lucro Arbitrado'].includes(doc.regime_tributario)",
            "description": "Alíquotas padrão para empresas do Lucro Real/Presumido"
        },
        {
            "fieldname": "aliquota_icms_padrao",
            "fieldtype": "Percent",
            "label": "Alíquota ICMS Padrão",
            "default": 18,
            "description": "Alíquota interna do ICMS do estado"
        },
        {
            "fieldname": "aliquota_pis",
            "fieldtype": "Percent",
            "label": "Alíquota PIS",
            "default": 1.65,
            "description": "PIS não-cumulativo: 1,65% | Cumulativo: 0,65%"
        },
        {
            "fieldname": "aliquota_cofins",
            "fieldtype": "Percent",
            "label": "Alíquota COFINS",
            "default": 7.6,
            "description": "COFINS não-cumulativo: 7,6% | Cumulativo: 3%"
        },
        {
            "fieldname": "column_break_impostos",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "aliquota_irpj",
            "fieldtype": "Percent",
            "label": "Alíquota IRPJ",
            "default": 15,
            "description": "Alíquota base do IRPJ (15%) + Adicional 10% sobre excedente"
        },
        {
            "fieldname": "aliquota_csll",
            "fieldtype": "Percent",
            "label": "Alíquota CSLL",
            "default": 9,
            "description": "Contribuição Social sobre Lucro Líquido"
        },
        {
            "fieldname": "aliquota_iss",
            "fieldtype": "Percent",
            "label": "Alíquota ISS",
            "description": "Alíquota do ISS para prestadores de serviço"
        },
        {
            "fieldname": "section_simples",
            "fieldtype": "Section Break",
            "label": "Configuração Simples Nacional",
            "depends_on": "eval:['1 - Simples Nacional', '2 - Simples Nacional - Excesso de Sublimite', '6 - MEI - Microempreendedor Individual'].includes(doc.regime_tributario)",
            "description": "Configurações específicas para empresas do Simples Nacional"
        },
        {
            "fieldname": "anexo_simples",
            "fieldtype": "Select",
            "label": "Anexo do Simples Nacional",
            "options": "\nAnexo I - Comércio\nAnexo II - Indústria\nAnexo III - Serviços\nAnexo IV - Serviços\nAnexo V - Serviços",
            "description": "Anexo da Lei Complementar 123/2006"
        },
        {
            "fieldname": "faixa_simples",
            "fieldtype": "Select",
            "label": "Faixa do Simples",
            "options": "\n1ª Faixa - Até R$ 180.000,00\n2ª Faixa - De R$ 180.000,01 a R$ 360.000,00\n3ª Faixa - De R$ 360.000,01 a R$ 720.000,00\n4ª Faixa - De R$ 720.000,01 a R$ 1.800.000,00\n5ª Faixa - De R$ 1.800.000,01 a R$ 3.600.000,00\n6ª Faixa - De R$ 3.600.000,01 a R$ 4.800.000,00",
            "description": "Faixa de faturamento dos últimos 12 meses"
        },
        {
            "fieldname": "column_break_simples",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "aliquota_simples",
            "fieldtype": "Percent",
